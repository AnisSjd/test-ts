{
  "include": ["./**/*"],
  "compilerOptions": {
    "strict": false,
    "allowJs": true,
    "checkJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": false,
    "target": "es6",
    "module": "ES6",
    "skipLibCheck": true
  }
}
